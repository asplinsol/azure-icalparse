{
  "name": "ical-to-json",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npm run clean && tsc",
    "package": "copyfiles --flat src/*.json dist/icalparser  && copyfiles host.json package.json dist && npm run zip",
    "zip": "npm-build-zip --source=dist --info=true",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "ical": "^0.6.0"
  },
  "devDependencies": {
    "@types/ical": "^0.6.1",
    "@azure/functions": "^1.0.2-beta2",
    "@types/node": "^13.5.0",
    "npm-build-zip": "^1.0.2",
    "copyfiles": "^2.1.1",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.5"
  }
}
